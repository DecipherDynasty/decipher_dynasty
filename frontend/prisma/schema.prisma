generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Account {
  email           String
  hashed_password String
  id              String @id @default(uuid())
  userAccountId   String @unique
  User            User   @relation(fields: [userAccountId], references: [accountId])

  @@map("account")
}

model User {
  accountId     String   @id @unique
  authorisation String
  full_name     String
  Account       Account?
}
